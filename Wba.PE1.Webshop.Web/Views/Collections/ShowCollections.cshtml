@model pe1.Webshop.Web.ViewModels.CollectionsDetailVm

@{
    Layout = "_Layout2";
    ViewData["Title"] = "View collections";
    ViewBag.Title = "Collections";
}


<div class="row justify-content-around align-baseline">

    @{
        foreach (var item in Model.Items)
        {

            <partial name="ShowItemsPartial" model="item" />
        }

    }
</div>


<p>
    <a href="/collections/all" class="btn btn-primary">&laquo; Back</a>
</p>
